<div>
    
        <div class="modal-header">
            <h3 class="modal-title">Select Commands</h3>
        </div>
        <div class="modal-body">
            
            <h1>Fetch commands</h1>


              <div class="row">
                                        <div class="col-md-12 margin_top" >

                                            <div id="firstGrid" ui-grid="createTestPlanOptions" ui-grid-selection ui-grid-cellNav class="grid"></div>
                                        </div>
                                    </div>
        </div>
        <div class="modal-footer">
            <button class="btn btn-primary" type="button" ng-click="addCommand()">OK</button>
            <button class="btn btn-primary" type="button" ng-click="closePopup()">Cancel</button>
        </div>
    
    
</div>