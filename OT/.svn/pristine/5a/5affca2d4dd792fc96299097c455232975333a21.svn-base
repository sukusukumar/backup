<!--New html-->
<div class="maincontainer" style="padding:15px">
    <div class="maincontent"     style="margin-left: 0px;">
        <div class="container-fluid">
            <div class="ot-deviceData" style="max-width:100%;margin-left:0px">

                <div class="tab-content">
                    <div id="devices" class="tab-pane fade in active">
                        <div class="orchestra_devices_table" style="height:800px;max-width:61%">
                            <div class="row">
                                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="max-width:100%">
                                    <div class="device-table">
                                        <div style="width:100%;height:50px">
                                            <h1><strong>Test Plan</strong></h1>
                                        </div>
                                        <div style="width:100%;height:50px;">
                                            <span><strong>Text_BNF</strong></span>
                                            <input type="text" name="text"></input>
                                        </div>
                                        <div class="tree" style="width: 100%;height: 500px;">
                                            <div class="table-responsive" style="width:48%;height:450px;float:left;background-color: grey;">
                                                <table class="table" style="width:100%">
                                                    <thead>
                                                        <tr>

                                                            <th><label class="hide" for="deviceID">Command Name </label><input type="text" class="device_col_search" id="deviceID" name="deviceId"></th>
                                                            <th><label class="hide" for="deviceName">Command Category</label><input type="text" class="device_col_search" id="deviceName" name="deviceName"></th>
                                                        </tr>
                                                        <tr>

                                                            <th>Command Name </th>
                                                            <th>Command Category </th>

                                                        </tr>

                                                    </thead>
                                                    <tbody  style="max-height:500px" tree-dnd-nodes>
                                                        <tr   ng-repeat="node in Commands"  drag-enabled="true">

                                                            <td>{{node.commandName}}</td>
                                                            <td>{{node.commandCategoryType}}</td>

                                                        </tr>                                                      
                                                        
                                                    </tbody>
                                                </table>
                                            </div>

                                            <div class="table-responsive" style="width:48%;height:450px;float:right;margin-left:25px;background-color:grey">
                                                <table class="table" style="width:100%">
                                                    <thead>
                                                        <tr>

                                                            <th style="width:60%">Name</th>
                                                            <th style="width:15%">Loop </th>
                                                            <th style="width:15%">Seq No</th>
                                                            <th> </th>

                                                        </tr>

                                                    </thead>
                                                    <tbody style="max-height:500px">

                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>							


                                    </div>

                                    <div style="width:100%;height:100px;float: right;margin-right:0px">
                                        <input type="button"  name="button" value="Next" class="btn btn-info" style="float: right;margin: 10px;width: 100px;"/>
                                        
                                    </div>


                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="menu2" class="tab-pane fade">
                        <h3>Menu 2</h3>
                        <p>Some content in menu 2.</p>
                    </div>
                </div>				
            </div>

        </div>
    </div>
</div>




