	.table-emailAdd,.table-emailAdd:hover,.table-emailAdd:focus{background:#fff;border-radius:4px;border:1px solid #00bfe9;padding: 12px 14px 9px;position: absolute;right: 16px;top: 0px;color:#00bfe9;line-height:10px;font-size:25px;text-decoration:none;outline:0;}
.ot-userAdministration .emailAdd{ max-width: 100%;right: 26px;top: 41px;width: 91%;}	
.popup-AdministrationAdd.emailAdd .form-group {display:block;}
.popup-AdministrationAdd.emailAdd .form-group:last-child {margin-bottom:0px;text-align:center;}
.ot-userAdministration .form-horizontal .popup-AdministrationAdd.emailAdd .form-group > label{padding-right:15px;}

.ot-toggle{margin:85px auto;}
.assign-table div .ot-toggle{margin-top:80px;}
.assign-table div .ot-toggle div{display:table;margin:0px auto;}
.ot-toggle button{ background-color: #ffffff;border: 1px solid #7eb2cf;border-radius: 4px;color: #7eb2cf;float: left;font-size: 20px;height: auto;line-height: 20px;margin-bottom: 10px;padding: 4px 10px 1px;}
.assign-table{max-width:800px;}
.assign-tableTitle{margin-top:10px;}
.assign-tableTitle .groupname{font-family:"Rajdhani-bold";}
.assign-table p{margin:10px 0px;}
.assign-table .assign-tableTitle p{margin-top:20px;}
.assign-table div{padding:0px;margin:0px;}
.assign-table div.ot-center{margin-left:10%;}
.assign-table div .ot-assignCenter{margin-left:18%;}
/* ot-userAdministration End
---------------------------------------------------------------*/
/*** Start of plan-section ***/
.plan-section{height:100%;}
.plan-section > .nav.nav-tabs > li,.plan-section > .nav.nav-tabs > li:last-child{width:50%;}
.plan-sectionBody {margin: 0 auto;text-align: center;}


.plan-sectionBody .plan-sectionBodyTitile{font-family:"Rajdhani-Bold";margin-bottom:0px;font-size:18px;}
.plan-sectionBody .ot-btn-data{margin-top:70px;}
.plan-sectionBody .plan-radioData{float:left;margin-right:20px;}
.plan-sectionBody input[type="radio"]{display: none;}
.plan-sectionBody .radio{padding-left:10px;}
.plan-sectionBody .radio label::before {background-color: #fff;bottom: 1px;content: "";display: inline-block;height: 17px;left: 0;margin-right: 10px;position: absolute;width: 17px;border-radius: 50%;margin-left: 0px;
}
.plan-sectionBody .plan-radioTitle {float: right;margin-left:5px;font-family:"Rajdhani-SemiBold";font-size:16px;color:#000;}
.plan-sectionBody .plan-radioTitle.active {font-family:"Rajdhani-Bold";color:#0e74c2;}
.plan-sectionBody .radio label {cursor: pointer;display: inline-block;font-size: 13px;padding-left: 20px;position: relative;margin-bottom:0px;}
.plan-sectionBody .radio label::after {background-color: #1276b8;border-radius: 50%;content: " ";display: inline-block;height: 7px;left: 5px;margin-left: 0px;position: absolute;top: 7px;transform: scale(0, 0);transition: transform 0.1s cubic-bezier(0.8, -0.33, 0.2, 1.33) 0s;width: 7px;
}
/*** END of plan-section ***/
/*** Start of user-administration ***/
.content-data{padding: 30px 80px;margin: 0 auto;}
.content-data p{font-weight: bold;}
.content-data table{border-collapse:collapse;float:left;margin-right:20px;}
.ot-userAdministration .user-form .form-horizontal .form-control{border-radius:2px;}
.user-form .bootstrap-select > .dropdown-toggle{border: 1px solid #b6b6b8; border-radius: 2px;}
.user-form{position:relative;clear:both;}
.plus-icon{ position: absolute;right: -11px;top: 62px;}
.user-form .dropdown-menu.customer-dropdown{top: 83px; right: -13px; padding: 40px 10px 20px;}
.user-form .dropdown-menu.customer-dropdown::before {border-bottom: 10px solid #ffffff;border-left: 10px solid rgba(0, 0, 0, 0);border-right: 10px solid rgba(0, 0, 0, 0);content: " ";position: absolute;right: 4px;top: -5px;}
.user-btns {display: table;margin: 20px auto; clear: both;}
.user-btns .ot-btn{margin:0 10px;padding: 3px 7px;}
.user-right-form{margin-top: 54px;}
/*** END of user-administration ***/

/*** Start of command parameter pop up ***/
#command-popup .modal-dialog{width:400px;}
#command-popup .bootstrap-select > .dropdown-toggle{width:42%;background:#F3F3F5;border:1px solid #cccccc;box-shadow:0 1px 1px rgba(0, 0, 0, 0.075) inset;}
#command-popup .form-control{background:#F3F3F5;}
#command-popup .modal-header{background:#ebf2fa;border-radius:4px 4px 0 0;}
#command-popup .modal-title{font-size:19px;font-weight:bold;text-align:center;}
#command-popup .user-form p{font-size:18px;font-weight:bold;text-align:center;}
/*** END of command parameter pop up ***/

/*** Start of schedule ***/
.schedule-form form{width:500px;margin:0 auto;clear:both;}
.schedule-form .form-control{background:#F3F3F5;}
.schedule-form form  > div{box-shadow:0 0 8px 0 #cccccc;padding:15px;border-radius:2px;}
.schedule-form .bootstrap-select:not([class*="col-"]):not([class*="form-control"]):not(.input-group-btn){width:100%;background:#F3F3F5;}
.schedule-form .user-btns{margin:80px auto;}
#start_dt,#end_dt{background-image:url("../images/calendar.png");background-position: right 10px center;background-repeat: no-repeat;}
.ui-widget-header .ui-icon{background-image:url("../images/ui-icons_454545_256x240.png")}
.schedule-form input::-webkit-input-placeholder {color: #1278BE;font-weight:bold;}
.schedule-form input:-moz-placeholder {color: #1278BE;font-weight:bold;}
.schedule-form input::-moz-placeholder {color: #1278BE;font-weight:bold;}
.schedule-form input:-ms-input-placeholder {color: #1278BE;font-weight:bold;}
/*** END of schedule ***/

.content-data.test-plan-data table{width:100%;margin-bottom:20px;}
.content-data caption{text-align:center;color:#000;font-weight:bold;}

