/* Task popup
---------------------------------------------------------------*/
.add_task_header h2{font-size: 20px;margin: 0px;font-family:"Rajdhani-Medium"}
.add_task_close{position: absolute;color: #fff;right: 30px;font-size: 30px;font-weight: bold;top:5px;cursor:pointer;}

.add_taskTab{border-bottom: 1px dashed #dddddd;}
.add_taskTab > li{text-align: center;margin-bottom: 0px;border-right: 1px dashed #dddddd;padding-top: 15px;width:25%;}
.add_taskTab > li:last-child{border-right: 0px dashed #dddddd;}
.add_task_inner .add_taskTab > li > a{padding:0px;font-size: 16px;margin:0px;font-family:"Rajdhani-SemiBold";color:#555555;}
.add_task_inner .add_taskTab > li > a > p{margin: 0 0 5px;}
.nav.add_taskTab > li > a:hover,.nav.add_taskTab > li > a:focus{background-color: #fff;border-color: #fff;}
.add_taskTab > li.active > a,.add_taskTab > li.active > a:hover,.add_taskTab > li.active > a:focus{border-color:transparent;}
.add_taskTab > li.disable ,.add_taskTab > li.disable:hover,.add_taskTab > li.disable:focus{opacity:.5;}
.add_task_badge{border-radius: 50%;color: #ffffff;display: inline-block;height: 20px;width: 20px;background-color:#00bfe9;font-size: 15px;font-family:"Rajdhani-SemiBold";margin-bottom: 10px;}

.task_title{font-size: 18px;color:#00bfe9;font-family:"Rajdhani-Medium"}
.task_control{border-radius: 0px;margin-bottom: 10px;font-size: 14px;font-family:"Rajdhani-Medium";resize: none;border-color: #d6d6d6;}

