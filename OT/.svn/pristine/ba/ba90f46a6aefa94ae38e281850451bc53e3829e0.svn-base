/* Device Styles
---------------------------------------------------------------*/
.orchestra_devices_table {max-width: 1020px; margin: 0 auto;}

.table-header {background-color: #fff; width:100%}
.table-header p {color: #2f3b4c; font-family: "Rajdhani-SemiBold"; font-size: 25px; float: left; padding: 6px 0 3px 10px; margin: 0;}

.device-header-section{ float:right; }
.device-header-section .dropdown{ float:left; padding:6px 12px; font-size:22px; color:#00bfe8; cursor:pointer; font-family: "Rajdhani-Medium"; margin-right:0px; }
.device-header-section .dropdown .arrow-icon{ background:url('../images/dropdown-blue.png'); width:14px; height:8px; display:inline-block; margin-left:54px; }

.device-header-section .device-menu{ overflow-x: auto;border-radius: 0px;border:none;padding:0px; max-height: 545px; margin: 0;}
.device-header-section .dropdown-menu .device-menu > li > a{padding: 10px 14px;background-color:#fff;display: block;color:#3c3c3c;font-size: 16px;}
.device-header-section .dropdown-menu .device-menu > li > a:hover{text-decoration: none;background-color: #d7dfec;}

.device-header-section .device-header-buttons{ float:left; }
.device-header-buttons a {background-color: #00bfe9; float: left; margin-left: 1px; padding: 12px 16px;}

.device-table{ /*height: 545px;*/ overflow: auto; }
.device-table .table-responsive {margin-bottom: 0;}
.device-table.job-table {height: 292px;}

.device-table .table  {margin: 0;}
.device-table .table thead {}
.device-table .table thead {background-color: #d7dfec; color: #2f3b4c; font-family: "Rajdhani-SemiBold"; font-size: 16px;}
.device-table .table thead tr th {border: none; border-bottom: 1px solid #fff; position: relative;}
.device-table .table thead tr th {border-left: 1px solid #fff;}
.device-table .table thead tr th span.caret {float: right; margin-top: 8px;}
.device-table .table thead tr th:first-child {border-left: 0;}
.device-table .table thead tr:first-child {cursor: pointer;}
.device-table .table thead tr:nth-child(2) {background-color: #ebf2fa;}

input.device_col_search {background-image: url('../images/device-search-icon.png'); background-repeat: no-repeat; background-position: left center; border: 0; background-color: transparent; font-family: "Rajdhani-Regular"; font-size: 16px; float: left; padding: 0 0 0 20px; width: 90%; outline: none;}

.device-table .table tbody { background-color: #ffffff; color: #78909c; font-family: "Rajdhani-Medium"; font-size: 16px; }
.device-table .table tbody tr td {border-color: #eeeff4; position: relative;}
.device-table .table tbody tr td {border-left: 1px solid #eeeff4;}
.device-table .table tbody tr td:first-child {border-left: 0;}
.device-table .table tbody tr td:last-child span {float: left;}
.device-table .table  a.device_settings {float: right; padding: 0 7px;}

.device-table .checkbox, .device-table .radio{ display:inline-block; }
.device-table .checkbox, .device-table .radio{ margin:0; padding-left:10px; }
.device-table .checkbox input[type="checkbox"],
.device-table .checkbox-inline input[type="checkbox"],
.device-table .radio input[type="radio"], .radio-inline input[type="radio"]{ position:static; margin:0; }

.device-table .checkbox label::before{ border-color:#00bfe9; }
.device-table .checkbox input[type="checkbox"]:checked + label::after{ color:#00bfe9; }

.orchestra_devices_table .table-more-info{ margin-top:15px; padding-left:100px; padding-right:100px; font-size:16px; }
.orchestra_devices_table .table-more-info .dl-horizontal{ margin-bottom:0; }
.orchestra_devices_table .table-more-info .dl-horizontal dt{ text-align:left; width:100px; float:left; margin-bottom:6px; font-family:"Rajdhani-SemiBold"; }
.orchestra_devices_table .table-more-info .dl-horizontal dd:before{ content:":"; font-weight:bold; display:inline; padding-right:15px; font-family:"Rajdhani-Medium" }
.orchestra_devices_table .table-more-info .dl-horizontal dd{ margin-left:120px; color:#78909c; }

.settings-in-open{ margin:0; padding:0; list-style:none; position:absolute; right:0; top:0; background-color:#ffffff; display:none; min-width: 398px; float: right;}
.settings-in-open li { float:left; margin-left:1px; }
.settings-in-open a{ width: 70px; padding: 8px 0; display:block; background-color:#00bfe9; text-align:center; }
.settings-in-open a.icon-close{ background-color:#d7dfec; width: 40px;}

.logs {}
.logs p {font-size: 18px; font-family: "Rajdhani-Regular"; line-height: 28px; padding: 8px 20px 3px 10px;}

.spinner { margin: 5px 30px 0 0; width: 230px;}
.spinner input {text-align: right;}
.input-group-btn-vertical {position: relative; white-space: nowrap; width: 1%; vertical-align: middle; display: table-cell;}
.input-group-btn-vertical > .btn {display: block; float: none; width: 100%; max-width: 100%; padding: 16px 12px 0px; margin-left: -1px; position: relative; border-radius: 0; outline: none;}
.input-group-btn-vertical > .btn:hover, .input-group-btn-vertical > .btn:focus {outline: none; background-color: transparent; box-shadow: none;}
.input-group-btn-vertical > .btn:first-child {border-top-right-radius: 4px;}
.input-group-btn-vertical > .btn:last-child {margin-top: -1px; border-bottom-right-radius: 4px; border-top: 0; padding: 0 12px 16px;}
.input-group-btn-vertical i {position: absolute; top: 0; left: 50%; margin-left: -4px;}

.jobs-device-table {height: 350px;}
.device-in-open {min-width: 254px;}


