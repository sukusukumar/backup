/* Global Styles
---------------------------------------------------------------*/
body {background: #edeef2; font-family: "Rajdhani-Regular";}
img {max-width: 100%; height: auto;}
.boxshdw {box-shadow: 0 2px 10px #e7e7e7;}
.magin-btm-20{margin-bottom: 20px;}
.topoffset-30 {margin-top: 30px;}
/* CSS 3.0 Custom Fonts
---------------------------------------------------------------*/

/* ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ */
/* ## The below styles will apply Custom font "Rajdhani-Bold, Rajdhani-Light, Rajdhani-Medium, Rajdhani-Regular, Rajdhani-SemiBold" to the text## */
/* ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ */
/* Rajdhani */
@font-face {
font-family: "Rajdhani-Bold";
src: url('../fonts/Rajdhani-Bold.eot');
src: url('../fonts/Rajdhani-Bold.eot?#iefix') format('embedded-opentype'), url('../fonts/Rajdhani-Bold.woff') format('woff'), url('../fonts/Rajdhani-Bold.ttf') format('truetype');
}
@font-face {
font-family: "Rajdhani-Light";
src: url('../fonts/Rajdhani-Light.eot');
src: url('../fonts/Rajdhani-Light.eot?#iefix') format('embedded-opentype'), url('../fonts/Rajdhani-Light.woff') format('woff'), url('../fonts/Rajdhani-Light.ttf') format('truetype');
}
 @font-face {
font-family: "Rajdhani-Medium";
src: url('../fonts/Rajdhani-Medium.eot');
src: url('../fonts/Rajdhani-Medium.eot?#iefix') format('embedded-opentype'), url('../fonts/Rajdhani-Medium.woff') format('woff'), url('../fonts/Rajdhani-Medium.ttf') format('truetype');
}
 @font-face {
font-family: "Rajdhani-Regular";
src: url('../fonts/Rajdhani-Regular.eot');
src: url('../fonts/Rajdhani-Regular.eot?#iefix') format('embedded-opentype'), url('../fonts/Rajdhani-Regular.woff') format('woff'), url('../fonts/Rajdhani-Regular.ttf') format('truetype');
}
 @font-face {
font-family: "Rajdhani-SemiBold";
src: url('../fonts/Rajdhani-SemiBold.eot');
src: url('../fonts/Rajdhani-SemiBold.eot?#iefix') format('embedded-opentype'), url('../fonts/Rajdhani-SemiBold.woff') format('woff'), url('../fonts/Rajdhani-SemiBold.ttf') format('truetype');
}

/* Header Styles
---------------------------------------------------------------*/
header {background-color: #fff; margin-bottom: 10px; padding: 3px 0;}
.orchestra_header .logo  {overflow: hidden; position: relative;}
.orchestra_header .logo a {height: auto;}

.orchestra_header .logo a.menu_dropdown {position: absolute; left: 15px; top: 15px; width: 42px; height: 45px;}
.ImageIcon{height:46px;}
.SettingsIcon{height:17px;}
.MoreMenuIcon{height:17px;}
.OrchestraLogo{height:36px;}
/* Menu Styles
---------------------------------------------------------------*/
.orchestra_menu {background-color: #fff; width: 214px; float: left; position: absolute; top: 70px; box-shadow: -1px 0 10px 0 rgba(0, 0, 0, 0.250);border:1px solid #D4D4D4;border-right:3px solid #D4D4D4;border-top:1px solid #D4D4D4;display: none; z-index: 999;margin-left:5px}
.orchestra_menu:before {content:' '; border-left: 10px solid transparent; border-right: 10px solid transparent; border-bottom: 10px solid #fff; position: absolute; top: -10px; left: 33px;}
.orchestra_menu ul li a {color: #000; font-family: "Rajdhani-Bold"; font-size: 14px; padding: 12px 12px 6px 16px;/*  border-top: 1px solid #f0eeef; */}
.orchestra_menu ul li a img {float: left; margin-right: 12px;margin-top:5px;}
.orchestra_menu ul li a span.link_name {line-height: 26px;font-size:16px;}
.orchestra_menu ul li a em {color: #00bfe9; float: right; font-style: normal;font-size: 14px; margin-top: 4px;color:#008000;}
.orchestra_menu ul li a:hover, .orchestra_menu ul li a:focus, .orchestra_menu ul li a:active {background-color: #fff;}

.orchestra_menu ul li ul.sub-nav {/* border-top: 1px solid #f0eeef; */ display: none;}
.orchestra_menu ul li ul.sub-nav li {margin-left: 40px;clear:both;}
.orchestra_menu ul li ul.sub-nav li a {color: #646464; font-family: "Rajdhani-Medium"; font-size: 13px; border-top: none`; border-bottom: 0; padding: 10px 0px 10px 16px;float:left;}
.orchestra_menu ul li ul.sub-nav li:first-child a {border: 0;}

.orchestra_menu ul li ul.map-subnav-icons li .subnavIcon {background-image: url('../images/fav_icon.png'); background-repeat: no-repeat; background-position: right 12px center; background-size: 10px 10px;float:right;margin-top:10px;padding:12px;}

.right_menu {float: right;}
.right_menu ul.nav {float: left; margin-top: 10px;}
.right_menu ul.nav li {float: left;}
.right_menu ul.nav li a {margin: 12px; padding: 0;}
.right_menu ul li.open > a, .right_menu ul li.open > a:hover, .right_menu ul li.open > a:focus {background-color: #fff;}
.right_menu ul.nav li a:hover, .right_menu ul li a:focus, .right_menu ul li a:active, .right_menu ul li a:visited {background-color: #fff;}

.right_menu .dropdown-menu {box-shadow: 0 -1px 10px 5px rgba(0,0,0,.175); border: 0; border-radius: 0; left: auto; margin-top: 16px; padding: 0; right: 0;}
.right_menu .dropdown-menu:before {content:' '; border-left: 7px solid transparent; border-right: 7px solid transparent; border-bottom: 7px solid #fff; position: absolute; top: -7px; right: 12px;}

.dropdown-menu ul.more_menu {padding: 0 10px; width: 240px;}
.dropdown-menu ul.more_menu li {margin-bottom: 10px; text-align: center; width: 110px;}
.dropdown-menu ul.more_menu li a {display: inline-block; text-align: center;}
.dropdown-menu ul.more_menu li a span {color: #646464; display: block; font-family: "Rajdhani-SemiBold"; font-size: 14px; line-height: 14px; margin-top: 8px;}

.dropdown-menu ul.more_menu li.users a span {margin-top: 10px;}
.dropdown-menu ul.more_menu li.groups a img {margin-top: 5px;}
.dropdown-menu ul.more_menu li.groups a span {margin-top: 15px;}

.dropdown-menu ul.settings_menu li.landing a span {margin-top: 12px;}

.user_profile {float: left; margin: 5px 0 0 20px;}
.user_profile a {display: block;}

.user_profile .dropdown-menu {margin-top: 8px; right: 14px;}
.user_profile .dropdown-menu:before {right: 28px;}

.user_profile_details {padding: 18px 15px 20px 12px; overflow: hidden;}
.profile_image {float: left; margin-right: 8px;}
.profile_details {float: left; margin-top: 10px;}
.profile_details p {color: #474848; display: block; font-family: "Rajdhani-Medium"; font-size: 14px; margin-bottom: 0;}
.profile_details span {color: #474848; display: block; font-family: "Rajdhani-Bold"; font-size: 14px;}
.profile_details a.my_account {background-color: #00bfe6; border-radius: 4px; color: #fff; display: inline-block; font-family: "Rajdhani-Semibold"; font-size: 14px; margin-top: 15px; line-height: 14px; padding: 9px 16px;}
.profile_details a.my_account:hover {text-decoration: none;}

.user_profile_operations {background-color: #edeef2; padding: 6px 15px 6px 12px;}
.user_profile_operations a.operation_btn {background-color: #fff; border-radius: 4px; box-shadow: 0 0 5px #ccc; color: #687a8c; display: inline-block; font-family: "Rajdhani-Semibold"; font-size: 14px; line-height: 14px; padding: 9px 9px 8px;}
.user_profile_operations a.operation_btn.signout {float: right;}
.user_profile_operations a:hover {text-decoration: none;}

/* Responsive Mobile Menu Styles
---------------------------------------------------------------*/
.mobile_menu_trigger {float: left; margin: 10px 0 0 5px;}
.mobile_menu_trigger div.navbar-default {border: 0; float: left;}

.mobile_menu {float: left; display: none; width: 100%;}
.navbar-default {border: 0;}
.navbar-header {background-color: #fff;}
.navbar-toggle {float: left;}

.orchestra_mobile_menu {background-color: #fff;}
.orchestra_mobile_menu ul li a {color: #000; font-family: "Rajdhani-SemiBold"; font-size: 14px; padding: 12px 12px 6px 16px; border-top: 1px solid #f0eeef;}
.orchestra_mobile_menu ul li a img {float: left; margin-right: 12px;}
.orchestra_mobile_menu ul li a span.link_name {line-height: 26px;}
.orchestra_mobile_menu ul li a em {color: #00bfe9; float: right; font-style: normal; font-family: "Rajdhani-SemiBold"; font-size: 30px; margin-top: -5px;}
.orchestra_mobile_menu ul li a:hover, .orchestra_mobile_menu ul li a:focus, .orchestra_mobile_menu ul li a:active {background-color: #fff;}

.orchestra_mobile_menu ul li ul.sub-nav {border-top: 1px solid #f0eeef; display: none;}
.orchestra_mobile_menu ul li ul.sub-nav li {margin-left: 40px;}
.orchestra_mobile_menu ul li ul.sub-nav li a {color: #646464; font-family: "Rajdhani-Medium"; font-size: 13px; border-top: 1px solid #f0eeef; border-bottom: 0; padding: 10px 12px 10px 16px;}
.orchestra_mobile_menu ul li ul.sub-nav li:first-child a {border: 0;}

