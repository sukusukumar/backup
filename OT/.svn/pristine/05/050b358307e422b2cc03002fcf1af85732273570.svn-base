<div ng-include="'app/views/header.html'"></div>						
<div>
	<div class="maincontent">
		<div class="container-fluid">
			<div class="orchestra_devices_table">
				<div class="row">
					<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
						<div class="table-header">
							<p style="color:#337ab7;">Device Administration</p>
							<div class="clearfix"></div>
						</div>
						<div class="device-table server-table" style="position: relative;top: 10px;">
							<div class="table-responsive">
								<div ui-grid="deviceAdminGridOptions" ui-grid-selection class="grid" style="margin:0 auto;max-width:100%;"></div>
								
								<button type="button" class="btn btn-info ot-btn" ng-click = "approve()">Approve</button>
								<button type="button" class="btn btn-info ot-btn" ng-click = "reject()" >Reject</button>
								<button type="button" class="btn btn-info ot-btn" ng-click = "reject()" >Restart IAC</button>
								<button type="button" class="btn btn-info ot-btn" ng-click = "reject()" >Register IAC</button>
								<button type="button" class="btn btn-info ot-btn" ng-click = "reject()" >UnRegister IAC</button>
							</div>
						</div>
					</div>
				</div>
			</div>
			
			<div ng-include="'app/views/favourites.html'" style="position: relative;top: 30px;"></div>
		</div>
	</div>
</div>
