<div ng-include="'app/views/header.html'"></div>
      <div class="maincontainer">
         <div class="maincontent">
            <div class="container-fluid">
				<div class="content-shadow">	
               <div class="ot-userAdministration" >
                  <ul class="nav nav-tabs">
                     <li class="active"><a data-toggle="tab" href="">Available Reports</a></li>
                     
                  </ul>
                  <div class="tab-content">
                     <div id="User" class="tab-pane fade in active">
                        <div class="orchestra_devices_table">
                           <div class="row">
                              <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
								 <div class="table-header">
									<ul class="userAdministration-btn">
					<!--				  <li class="active"><a data-toggle="tab" href="#home">Username</a></li>  -->
									
									</ul>

									<div class="tab-content">
									  <div id="home" class="tab-pane fade in active">
										<div class="device-table">
									<div class="">
									<a href="" class="table-AdministrationAdd" ng-click="showAddReportDiv()"><img src="images/device-plus-icon.png" alt="add"/></a>
										
									</div>
                                    <div class="table-responsive">
									<div ui-grid="availableReportsGridOptions" ui-grid-selection class="grid" style="margin:0 auto;max-width:100%;"></div>

                                    </div>
                                 </div>
									  </div>
									 

									</div>
                                    <div class="clearfix"></div>
                                 </div>
                              </div>
                           </div>
                        </div>
                     </div>


                  </div>
               </div>
			   <div class="ot-userAdministration" ng-show="reportDiv"  style ="min-height:250px;" >
                  <ul class="nav nav-tabs" >
                     <li class="active"><a >Add Report</a></li>
                   
                  </ul>
                  <div class="tab-content">
                     <div id="account"  >
                        <div class="orchestra_devices_table">
                           <div class="row">
                              <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
									<form class="form-horizontal">
									 
												<div class="form-group">
													<label for="report_name" class="col-sm-4 control-label">Report Name</label>
													<div class="col-sm-4">
														<input type="text" class="form-control" id="report_name">
													</div>
												</div>
												<div class="form-group">
													<label for="report_url" class="col-sm-4 control-label">Report URL</label>
													<div class="col-sm-4">
														<input type="text" class="form-control" id="report_url">
													</div>
												</div>
												<div class="form-group">
													<label for="report_type" class="col-sm-4 control-label">Report Type</label>
													<div class="col-sm-4">
													<!--	<input type="select" class="form-control" id="report_type">		-->
														<select class="form-control" id="report_type">
															<option ng-repeat="data in menuTypeList">{{data.menuType}}</option>
														</select>
													</div>
												</div>
											
									  <div class="form-group ot-btn-data"> 
										<div class="col-sm-offset-2 col-sm-10  text-center">
										  <button type="button" class="btn btn-info ot-btn" ng-click="addNewReport()">Save</button>
										  <button type="button" class="btn btn-info ot-btn" ng-click="canceladdReport()">Cancel</button>
										</div>
									  </div>
									</form>
                              </div>
                           </div>
                        </div>
                     </div>
		
		
                  </div>
               </div>
				</div>
				<div ng-include="'app/views/favourites.html'"></div>
			</div>
         </div>
      </div>
	  <div ng-include="'app/views/favouritesPopUp.html'"></div>
	  <div ng-include="'app/views/popUp.html'"></div>