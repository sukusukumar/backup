/* Login Page
---------------------------------------------------------------*/

.orchestra_login{background-color: #cccccc; max-width:400px;box-shadow: 0 0 20px #767676;margin: 15% auto 20px;border-radius: 14px;text-align:center; font-family:arial,helvetica,sans-serif;}

.orchestra_loginHeader{background-color:#e6e6e6;padding: 18px 0px;border-radius:14px 14px 0px 0px;}
.orchestra_logincontent{padding: 23px 40px 0;}

.orchestra_logincontent .input-group{box-shadow:0px 1px 2px #ababab; margin-bottom:23px;width:100%;}
.input-group-addon,.input-group .form-control{height:54px;border-radius: 0px;font-size:16px;}
.input-group .form-control:focus{box-shadow:none;}
span.input-group-addon{ width:50px;max-width: 100%; height: auto;}
.orchestra_login .orchestra_loginfooter{
background: -moz-linear-gradient(top,  #7fd5f5 0%, #42b4ed 100%);
background: -webkit-linear-gradient(top,  #7fd5f5 0%,#42b4ed 100%);
background: -ms-linear-gradient(top,  #7fd5f5 0%,#42b4ed 100%); 
background: linear-gradient(to bottom,  #7fd5f5 0%,#42b4ed 100%); 
 margin-bottom:18px; border-radius: 0px 0px 14px 14px;padding: 23px 0px;display: inline-block; width:100%;
 border-bottom:1px solid #2a769c;}
 
.orchestra_login .btn-orchestraLogin,.btn-orchestraLogin:focus,.btn-orchestraLogin:active{
background: -moz-linear-gradient(top,  #bbc3c9 2%, #9fa9b3 3%, #72818f 100%); 
background: -webkit-linear-gradient(top,  #bbc3c9 2%,#9fa9b3 3%,#72818f 100%);
background: -ms-linear-gradient(top,  #bbc3c9 2%,#9fa9b3 3%,#72818f 100%);
background: linear-gradient(to bottom,  #bbc3c9 2%,#9fa9b3 3%,#72818f 100%);
border:1px solid #596670; color:#fff; text-shadow:0px 1px 0px #2a2a2a; padding:5px 40px;font-weight: bold;font-size: 15px;outline:none;}
.btn-orchestraLogin:hover{color:#42b4ed;}

.container-fluid .login-copyright{ positon:relative;font-family: arial, helvetica, sans-serif;text-align: center;}
